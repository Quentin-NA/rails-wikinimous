<h1>Here you can edit the article!</h1>
<br>
<div class="container">
<%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_area :content %>
  <%= f.submit %>
<% end %>
</div>

<p id="small"><%= link_to 'Show'.html_safe , article_path(@article) %>|<%= link_to "Back", articles_path %></p>